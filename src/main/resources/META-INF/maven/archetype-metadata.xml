<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="project"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet encoding="UTF-8">
      <directory>src/main/resources</directory>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>src/main/jnlp</directory>
    </fileSet>
    <fileSet encoding="UTF-8" filtered="true" packaged="true">
      <directory>src/main/scala</directory>
      <includes>
        <include>**/*.scala</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8" filtered="true" packaged="true">
      <directory>src/test/scala</directory>
      <includes>
        <include>**/*.scala</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>run-scripts/windows</directory>
      <includes>
        <include>**/*.bat</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>run-scripts/linux</directory>
      <includes>
        <include>**/*.sh</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>assembly.xml</include>
          <include>build.properties</include>
          <include>local-build.properties</include>
          <include>pom.xml</include>
          <include>readme_rus</include>
          <include>readme</include>
      </includes>
    </fileSet>
    <fileSet filtered="false" encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>my_keystore</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
